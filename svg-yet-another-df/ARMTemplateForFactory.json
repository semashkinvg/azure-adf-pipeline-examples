{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "svg-yet-another-df"
		},
		"svg_westus_datalakestorage_biz_data_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'svg_westus_datalakestorage_biz_data'"
		},
		"svg_westus_datalakestorage_biz_data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://svgwestusstagingbizdata2.dfs.core.windows.net/"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/svg_westus_datalakestorage_biz_data')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('svg_westus_datalakestorage_biz_data_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('svg_westus_datalakestorage_biz_data_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/biz_data_source')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "svg_westus_datalakestorage_biz_data",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "csv"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileSystem": "svgwestusstagingbizdata2"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/svg_westus_datalakestorage_biz_data')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/svg_westus_datalakestorage_biz_data_parquet')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "svg_westus_datalakestorage_biz_data",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "parquet"
				},
				"annotations": [],
				"type": "Parquet",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileSystem": "svgwestusstagingbizdata2"
					},
					"compressionCodec": "snappy"
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/svg_westus_datalakestorage_biz_data')]"
			]
		}
	]
}